{
  "name": "workflow-social-media-client",
  "version": "1.2.1",
  "description": "Workflow Course Assignment - Improve the quality of a package by establishing helpful workflows that make the development process more efficient",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "watch": "sass src/scss:dist/css --watch",
    "start": "live-server",
    "dev": "npm-run-all --parallel watch start",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install",
    "test": "npm run test-unit && npm run test-e2e-cli",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-cli": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EliseAur/workflow-social-media-client.git"
  },
  "keywords": [
    "workflow",
    "esLint",
    "Prettier",
    "Hooks",
    "Unit-testing",
    "e2e-testing"
  ],
  "author": "Noroff, Elise Aurtande",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^13.6.4",
    "dotenv": "^16.4.2",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.6.3",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-localstorage-mock": "^2.4.26",
    "lint-staged": "^15.2.1",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
